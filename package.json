{
  "name": "postact",
  "version": "0.0.0",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "docs:dev": "bun --bun vitepress dev docs",
    "docs:build": "bun --bun vitepress build docs",
    "docs:preview": "bun --bun vitepress preview docs",
    "build": "bun run build.js",
    "fmt": "oxfmt '!**/dist/**/*' '!**/*.{json,md,html}' packages/"
  },
  "dependencies": {
    "@postact/core": "workspace:*",
    "@postact/jsx": "workspace:*",
    "@postact/client-router": "workspace:*",
    "@postact/serde": "workspace:*",
    "@postact/ssr": "workspace:*"
  },
  "devDependencies": {
    "@shikijs/vitepress-twoslash": "^3.21.0",
    "@types/bun": "latest",
    "@vitest/ui": "^4.0.18",
    "@vue/compiler-dom": "^3.5.27",
    "@vue/runtime-dom": "^3.5.27",
    "@vue/server-renderer": "^3.5.27",
    "@vue/shared": "^3.5.27",
    "jsdom": "^27.4.0",
    "oxfmt": "^0.24.0",
    "remark-shiki-twoslash": "^3.1.3",
    "rolldown": "^1.0.0-rc.1",
    "shiki": "^3.21.0",
    "vite": "^7.3.1",
    "vitepress": "^2.0.0-alpha.15"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "workspaces": [
    "packages/**/*"
  ]
}
