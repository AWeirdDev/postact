{
  "name": "@postact/jsx",
  "version": "0.0.9",
  "description": "JSX support for Postact.",
  "author": {
    "name": "AWeirdDev",
    "email": "awdjared@gmail.com",
    "url": "https://github.com/AWeirdDev"
  },
  "repository": {
    "url": "https://github.com/AWeirdDev/postact"
  },
  "license": "MIT",
  "type": "module",

  "exports": {
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js",
      "default": "./dist/jsx-dev-runtime.js"
    }
  },
  "scripts": {
    "build": "tsc --build --clean && tsc --build && rolldown -m -c",
    "fmt": "oxfmt jsx-runtime.ts jsx-dev-runtime.ts test.tsx"
  },
  "dependencies": {
    "@postact/core": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "oxfmt": "^0.24.0"
  },
  "files": [
    "./src/**/*",
    "./dist/*"
  ],
  "peerDependencies": {
    "typescript": "^5"
  },
  "private": false
}
